def binary_file_to_scaled_values(file_path, output_file):
    with open(file_path, "r") as file:
        binary_values = file.readlines()

    # Convert binary strings to integers and divide by 256
    scaled_values = [str(int(line.strip(), 2) / 256) for line in binary_values]

    # Save scaled values to an output file
    with open(output_file, "w") as file:
        file.write("\n".join(scaled_values))

    return scaled_values

# Example usage
input_file = "image_mem6x6.memfile"  # Replace with your input file path
output_file = "image_mem6x6_fp.txt"  # Output file

scaled_values = binary_file_to_scaled_values(input_file, output_file)